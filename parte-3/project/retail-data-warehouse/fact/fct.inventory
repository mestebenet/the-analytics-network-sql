DROP TABLE IF EXISTS fct.inventory;

CREATE TABLE IF NOT EXISTS fct.inventory
(
    date date,
    store_id smallint,
    item_id character varying(10) COLLATE pg_catalog."default",
    initial smallint,
    final smallint
	constraint fk_product_id_inventory
		foreign key (store_id)
		references dim.store_master(store_id)
)
