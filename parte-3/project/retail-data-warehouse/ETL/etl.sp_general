CREATE OR REPLACE PROCEDURE etl.sp_general()
LANGUAGE plpgsql AS $$
BEGIN
  PERFORM etl.fct_return_movements_log();
  PERFORM etl.sp_dim_cost_log();
  PERFORM etl.sp_dim_employee_log();
  PERFORM etl.sp_dim_supplier_log();
  PERFORM etl.sp_fct_inventory_log();
  PERFORM etl.sp_fct_order_line_sale_log();
  PERFORM etl.sp_fct_store_traffic_log();
  PERFORM etl.sp_fx_rate_log();
  PERFORM etl.sp_product_master_log();
  PERFORM etl.sp_store_master_log();
  
END;
$$;
